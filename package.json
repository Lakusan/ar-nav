{
  "name": "ar-nav",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "server": "nodemon ./bin/www.js --ignore client",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node ./bin/www",
    "build": "npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-handlebars": "^5.1.0",
    "http-errors": "~1.6.3",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.5",
    "morgan": "~1.9.1",
    "node-geocoder": "^3.27.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
